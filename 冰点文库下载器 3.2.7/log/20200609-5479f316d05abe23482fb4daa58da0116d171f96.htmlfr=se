!function(){    require.async(['wkcommon:widget/ui/lib/jquery/jquery.js', 'wkcommon:widget/vipNoAdDialog/vipNoAdDialog.js', 'wkcommon:widget/ui/lib/cookie/cookie.js', 'wkcommon:widget/ui/lib/hxABtest/hxABtest.js', 'wkcommon:widget/lib/fis/data/data.js'], function($, NoAdDialog, Cookie, HxABtest,Data) {
        if ($('body .ad-logo').length > 0) {
            $('body .ad-logo').each(function () {
                $(this).click(function () {
                    // 大会员关闭广告
                    if (+$('#jiaoyu-vip-data').val()) {
                        // 关闭广告开关
                        $('.ad-onff').remove();
                        // 当天24点，也就是第二日0点
                        // Cookie.zeroCookie('adOnView', '/view');
                        var WkInfo = Data.get('WkInfo') || {};
                        var docInfo = WkInfo.DocInfo || {};
                        var wkAdMid = WkInfo.WkAdInfo && WkInfo.WkAdInfo.wkAdMid && JSON.parse(WkInfo.WkAdInfo.wkAdMid);
                        var vipNoAd = (wkAdMid && wkAdMid.vipNoAd) || {};
                        HxABtest.setUpCookie('adOnView', '/view', {
                            vipNoAd: vipNoAd,
                            'jiaoyu_vip_stime': docInfo.jiaoyu_vip_stime
                        });
                    }
                    else {
                        new NoAdDialog();
                    }
                });
            });
        }
    })
}();
